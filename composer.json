{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "dev-carioca as 5.2.29.0",
        "tymon/jwt-auth": "0.5.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "barryvdh/laravel-debugbar": "^2.2",
        "stavarengo/php-sigep": "dev-carioca as 1.0.x-dev",
        "stavarengo/php-sigep-fpdf": "dev-master",
        "itbz/fpdf": "^1.7",
        "nfephp-org/nfephp": "dev-carioca",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "xethron/migrations-generator": "dev-l5",
        "way/generators": "dev-feature/laravel-five-stable"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev",
        "secure-http": false
    },
    "repositories": {
        "generators": {
            "type": "git",
            "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
        },
        "framework": {
            "type": "vcs",
            "url": "https://github.com/diego3g/framework.git"
        },
        "php-sigep": {
            "type": "vcs",
            "url": "https://github.com/diego3g/php-sigep.git"
        },
        "nfephp": {
            "type": "vcs",
            "url": "https://github.com/diego3g/nfephp.git"
        }
    }
}
