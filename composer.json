{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "vendor": "cariocacelulares",
    "authors": [
        {
            "name": "Cleiton Souza",
            "email": "cleiton7souza@gmail.com"
        },
        {
            "name": "Diego Schell Fernandes",
            "email": "diego.schell.f@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">= 5.5.9",
        "laravel/framework": "5.3.*",
        "tymon/jwt-auth": "0.5.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "stavarengo/php-sigep": "dev-carioca as 1.0.x-dev",
        "stavarengo/php-sigep-fpdf": "dev-master",
        "itbz/fpdf": "^1.7",
        "nfephp-org/nfephp": "dev-carioca",
        "zizaco/entrust": "5.2.x-dev",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "guzzlehttp/guzzle": "~6.0",
        "swiftmailer/swiftmailer": "^5.4",
        "venturecraft/revisionable": "1.*",
        "arcanedev/log-viewer": "4.0.x-dev",
        "spatie/browsershot": "^1.5",
        "maatwebsite/excel": "~2.1.0",
        "dompdf/dompdf": "~0.6.1",
        "vinelab/cdn": "^1.4",
        "sentry/sentry-laravel": "^0.4.1",
        "jenssegers/mongodb": "^3.1",
        "nwidart/laravel-modules": "1.14.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "xethron/migrations-generator": "dev-l5",
        "way/generators": "dev-feature/laravel-five-stable",
        "barryvdh/laravel-debugbar": "^2.2",
        "tightenco/mailthief": "^0.3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Tests\\": "tests/",
            "Bootstrap\\": "bootstrap/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev",
        "secure-http": false
    },
    "repositories": {
        "generators": {
            "type": "vcs",
            "url": "https://github.com/jamisonvalenta/Laravel-4-Generators.git"
        },
        "php-sigep": {
            "type": "vcs",
            "url": "https://github.com/diego3g/php-sigep.git"
        },
        "nfephp": {
            "type": "vcs",
            "url": "https://github.com/diego3g/nfephp.git"
        }
    }
}
